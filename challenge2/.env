# Example environment variables file for large AD deployment
# Copy this file to .env and fill in your values
# Run: cp .env.example .env

# Admin credentials
TF_VAR_admin_username=adminuser
TF_VAR_admin_password=ChangeThisToSecurePassword!

# Client credentials
TF_VAR_client_admin_username=clientadmin
TF_VAR_client_admin_password=ChangeThisToSecurePassword!

# Storage account information
TF_VAR_storage_account_name=yourstorageaccountname
TF_VAR_storage_account_key=yourstorageaccountkey

# Deployment configuration
TF_VAR_domain_name=vaxlabs.local
TF_VAR_netbios_name=VAXLABS
TF_VAR_location=eastus

# Resource counts (uncomment to override defaults)
# TF_VAR_dc_count=12
# TF_VAR_client_count=24
# TF_VAR_file_server_count=8